function _init()
	t=0
	p_ani={240,241,242,243}
	dir_x={-1,1,0,0}
	dir_y={0,0,-1,1}
	_upd=update_game
	_drw=draw_game
	start_game()
end

function _update60()
	t+=1
	_upd()
end

function _draw()
	_drw()
	draw_wind()
end

function start_game()
	butt_buf=-1
	p_x=1
	p_y=1
	p_ox=0
	p_oy=0
	p_sox=0
	p_soy=0
	p_flip=false
	p_mov=nil
	p_t=0
	wind={}

	add_wind(32,64,64,24,{"hello world","this is line 2"})	
end

