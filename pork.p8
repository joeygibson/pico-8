function _init()
	t=0
	dpal={0,1,1,2,1,13,6,4,4,9,3,13,1,13,14}
	dir_x={-1,1,0,0,1,1,-1,-1}
	dir_y={0,0,-1,1,-1,1,1,-1}

	mob_ani={240,192}
	mob_atk={1,1}
	mob_hp={5,2}
	
	_upd=update_game
	_drw=draw_game
	debug={}
	start_game()
end

function _update60()
	t+=1
	_upd()
	do_floats()
	do_hp_win()
end

function _draw()
	_drw()
	draw_wind()
	check_fade()
	cursor(4,4)
	color(8)
	for txt in all(debug) do
		print(txt)
	end
end

function start_game()
	fadeperc=1
	butt_buf=-1
	mob={}
	dmob={}
	p_mob=add_mob(1,1,1)

	for x=0,15 do
		for y=0,15 do
			if mget(x,y)==192 then
				add_mob(2,x,y)
				mset(x,y,1)
			end
		end
	end

	p_t=0
	wind={}
	float={}
	talk_wind=nil
	fog=blankmap(1)
	hp_wind=add_wind(5,5,28,13,{})
	_upd=update_game
	_drw=draw_game
	unfog()
end

