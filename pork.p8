version 39
__lua__
function _init()
	_upd=update_game
	_draw=draw_game
	start_game()
end

function _update()
	_upd()
end

function _draw()
	_drw()
end

function start_game()
	p_x=3
	p_y=5
end

-->8
-- updates
function update_game()	
	if btnp(⬅️) then
		p_x-=1
	end
	if btnp(➡️) then
		p_x+=1
	end
	if btnp(⬆️) then
		p_y-=1
	end
	if btnp(⬇️) then
		p_y+=1
	end
end

function update_gameover()
end

-->8
-- draws
function draw_game()
	cls()
	map()

	palt(0,false) -- mak black opaque
	pal(6,10)	-- make grey into yellow
	spr(240,p_x*8,p_y*8)
	pal() -- switch palette back
end

function draw_gameover()
end

