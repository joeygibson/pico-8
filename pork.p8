-- gameplay
function move_player(dx,dy)
	local dest_x,dest_y=p_x+dx,p_y+dy
	local tle=mget(dest_x,dest_y) -- get the map tile at that coordinate

	if dx<0 then
		p_flip=true
	elseif dx>0 then
		p_flip=false
	end

	if fget(tle,0) then
		-- wall
		p_sox,p_soy=dx*8,dy*8
		p_ox,p_oy=0,0
		p_t=0
		_upd=update_pturn
		p_mov=mov_bump
		if fget(tle,1) then
			trig_bump(tle,dest_x,dest_y)
		end
	else
		sfx(63)
		p_x+=dx
		p_y+=dy

		p_sox,p_soy=-dx*8,-dy*8
		p_ox,p_oy=p_sox,p_soy
		p_t=0
		_upd=update_pturn
		p_mov=mov_walk
	end
end

function trig_bump(tle,dest_x,dest_y)
	if tle==7 or tle==8 then
		-- vase
		sfx(59)
		mset(dest_x,dest_y,1)	-- replace tile with empty tile
	elseif tle==10 or tle==12 then
		-- chest
		sfx(61)
		mset(dest_x,dest_y,tle-1)
	elseif tle==13 then
		-- door
		sfx(62)
		mset(dest_x,dest_y,1)
	end	
end

