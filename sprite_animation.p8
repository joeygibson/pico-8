version 38
__lua__
sprite = 1
x = -8
y = 59
timing = 0.25

function _draw()
	cls()
	spr(sprite, x, y)
	print(sprite)
end

function _update()
	sprite += timing
	if sprite > 5 then sprite = 1 end
	
	x += 1
	if x > 127 then x = -8 end
end	
